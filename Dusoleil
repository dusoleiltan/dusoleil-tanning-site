7) assets/style.css
:root{
  --bg: #0b0b0b;
  --text: #1a1a1a;
  --muted: #6b6b6b;
  --brand: #d4a373;      /* warm bronze */
  --brand-dark: #A0631D;
  --card: #141414;
  --white: #ffffff;
  --radius: 12px;
}
* { box-sizing: border-box; }
html, body { margin: 0; padding: 0; font-family: "Poppins", system-ui, sans-serif; color: var(--text); background: #fff; }
.container { width: min(1100px, 90%); margin: 0 auto; }

.site-header { position: sticky; top: 0; z-index: 1000; border-bottom: 1px solid #eee; background: rgba(255,255,255,0.98); }
.header-inner { display: flex; align-items: center; justify-content: space-between; padding: 14px 0; }
.logo { font-weight: 800; font-size: 1.4rem; color: var(--brand-dark); text-decoration: none; letter-spacing: .5px; }
.primary-nav ul { list-style: none; display: flex; gap: 20px; padding: 0; margin: 0; }
.primary-nav a { text-decoration: none; color: var(--text); font-weight: 600; }
.primary-nav a.active, .primary-nav a:hover { color: var(--brand); }

.menu-toggle { display: none; background: #fff; border: 1px solid #ddd; padding: 8px 12px; border-radius: 6px; cursor: pointer; }

.hero { position: relative; color: white; min-height: 60vh; display: grid; align-items: center; background-size: cover; background-position: center; }
.hero .overlay { position: absolute; inset: 0; background: linear-gradient(to bottom right, rgba(0,0,0,.45), rgba(0,0,0,.25)); }
.hero-content { position: relative; z-index: 1; padding: 40px 0; text-align: left; }
.hero h1 { font-size: 2.5rem; margin: 0 0 12px; }
.hero p { font-size: 1.15rem; margin-bottom: 20px; max-width: 700px; }
.btn { display: inline-block; padding: 12px 20px; border-radius: 8px; text-decoration: none; font-weight: 700; }
.btn-primary { background: var(--brand); color: #fff; border: 0; }
.btn-secondary { background: #fff; color: var(--brand-dark); border: 2px solid var(--brand-dark); }

.intro { padding: 40px 0; }
.grid { display: grid; gap: 20px; }
.grid.three { grid-template-columns: repeat(3, 1fr); }
.grid.two { grid-template-columns: 1fr 1fr; }
.grid.four { grid-template-columns: repeat(4, 1fr); }
.card, .service-card, .price-card { background: #fff; border: 1px solid #eee; border-radius: var(--radius); padding: 20px; text-align: left; box-shadow: 0 2px 6px rgba(0,0,0,.04); }
.card .icon { font-size: 1.6rem; margin-bottom: 8px; }
.gallery-grid { grid-template-columns: repeat(4, 1fr); gap: 12px; }
.gallery-grid img { width: 100%; height: 130px; object-fit: cover; border-radius: 8px; }

.image { width: 100%; height: auto; }

.contact-form { display: grid; gap: 12px; max-width: 600px; }
.contact-form label { font-weight: 600; }
.contact-form input, .contact-form textarea { padding: 12px; border-radius: 8px; border: 1px solid #ddd; width: 100%; }
.booking-panel { border: 1px solid #ddd; padding: 20px; border-radius: 8px; background: #f9f9f9; }

